@model IEnumerable<El_Gran_PetCare.Models.VetClass>
<h2>El Gran Pet Care</h2>
<h3>Vet</h3>
<p>@Html.ActionLink("New Vet", "CreateVet")</p>
<table>
    <thead>
        <tr>
            <th>Vet ID</th>
            <th>Vet Name</th>
            <th>Specialisation</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var vet in Model)
        {
            <tr>
                <td>@vet.vetID</td>
                <td>@vet.vetName</td>
                <td>@vet.vetSpecialisation</td>
                <td>
                    <a href="@Url.Action("UpdateVet", "Home", new {@vetID=vet.vetID })">Edit</a>
                    <a href="@Url.Action("DeleteVet", "Home", new {@vetID=vet.vetID })">Delete</a>
                </td>
            </tr>
        }
    </tbody>

</table>
